{
  "name": "type-imagereader",
  "version": "1.0.0",
  "description": "A typscript powered web based image and comic reader pulling from local files",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts,.js",
    "start": "ts-node .",
    "pretest": "npm run build",
    "test": "nyc mocha",
    "knex": "knex --knexfile knexfile.ts",
    "predev": "npm run build",
    "dev": "DEBUG='type-imagereader:*' SKIP_SYNC=1 ts-node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AccaliaDeElementia/type-imagereader.git"
  },
  "author": "Accalia Elementia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AccaliaDeElementia/type-imagereader/issues"
  },
  "homepage": "https://github.com/AccaliaDeElementia/type-imagereader#readme",
  "dependencies": {
    "brfs": "^2.0.2",
    "browserify": "^17.0.1",
    "command-line-args": "^5.2.1",
    "common-shakeify": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "jet-logger": "^2.0.1",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "postgresql": "0.0.1",
    "pug": "^3.0.3",
    "sass": "^1.81.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "terser": "^5.36.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsify": "^5.0.4",
    "typescript": "^5.3.3",
    "words-to-numbers": "^1.5.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testdeck/mocha": "^0.3.3",
    "@types/browserify": "^12.0.40",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^7.1.8",
    "@types/command-line-args": "^5.2.3",
    "@types/cookie-parser": "^1.4.8",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/jsonfile": "^6.1.4",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/pug": "^2.0.10",
    "@types/serve-favicon": "^2.5.7",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdom": "^25.0.1",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "sinon": "^19.0.2",
    "source-map-support": "^0.5.21",
    "typescript-eslint": "^8.11.0"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "**/*.ts",
      "utils/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "knexfile.ts",
      "test.ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "all": true
  }
}
